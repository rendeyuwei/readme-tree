{
	"name": "readme-tree",
	"displayName": "readme-tree",
	"description": "auto generate directory tree in your readme",
	"version": "0.0.2",
	"publisher": "rende",
	"repository": {
		"type": "git",
		"url": "https://github.com/rendeyuwei/readme-tree"
	},
	"engines": {
		"vscode": "^1.66.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:readme-tree.GetDirTree"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "readme-tree.GetDirTree",
				"title": "Get Directory Tree"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "readme-tree.GetDirTree",
					"when": "resourceExtname == .md",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.66.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"eslint": "^8.11.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.2",
		"typescript": "^4.5.5",
		"@vscode/test-electron": "^2.1.3"
	}
}
